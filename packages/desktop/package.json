{
  "name": "@notecove/desktop",
  "version": "0.2.1",
  "description": "NoteCove Desktop App (Electron)",
  "main": "dist-electron/main/index.js",
  "author": "NoteCove Contributors",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "build:browser": "vite build --config vite.browser.config.ts && mv dist-browser/index-browser.html dist-browser/index.html",
    "build:all": "pnpm build && pnpm build:browser && electron-builder --mac --win --linux",
    "build:mac": "pnpm build && pnpm build:browser && electron-builder --mac",
    "build:win": "pnpm build && pnpm build:browser && electron-builder --win",
    "build:linux": "pnpm build && pnpm build:browser && electron-builder --linux",
    "preview": "electron-vite preview",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src",
    "pretest": "pnpm rebuild:node",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "pretest:e2e": "pnpm exec electron-rebuild -f -w better-sqlite3 && pnpm exec electron-rebuild -f -w @mongodb-js/zstd && pnpm exec electron-rebuild -f -w sharp",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "rebuild": "npx @electron/rebuild",
    "rebuild:node": "cd ../../node_modules/.pnpm/better-sqlite3@12.5.0/node_modules/better-sqlite3 && npm run build-release",
    "rebuild:electron": "pnpm exec electron-rebuild -f -w better-sqlite3 && pnpm exec electron-rebuild -f -w @mongodb-js/zstd && pnpm exec electron-rebuild -f -w sharp",
    "migrate:flag-byte": "tsx src/main/storage/migrate-flag-byte.ts"
  },
  "dependencies": {
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@floating-ui/dom": "^1.6.0",
    "@fastify/cors": "^11.1.0",
    "@fastify/static": "^8.3.0",
    "@fastify/websocket": "^11.2.0",
    "@minoru/react-dnd-treeview": "^3.5.3",
    "@mongodb-js/zstd": "^2.0.1",
    "@mui/icons-material": "^5.15.0",
    "@mui/material": "^5.15.0",
    "@mui/x-date-pickers": "^8.22.1",
    "@mui/x-tree-view": "^8.15.0",
    "@notecove/shared": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.207.0",
    "@opentelemetry/resources": "^2.2.0",
    "@opentelemetry/sdk-metrics": "^2.2.0",
    "@opentelemetry/sdk-node": "^0.207.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@tiptap/core": "3.14.0",
    "@tiptap/extension-code-block": "3.14.0",
    "@tiptap/extension-code-block-lowlight": "3.14.0",
    "@tiptap/extension-collaboration": "3.14.0",
    "@tiptap/extension-link": "3.14.0",
    "@tiptap/extension-list": "3.14.0",
    "@tiptap/extension-table": "3.14.0",
    "@tiptap/pm": "3.14.0",
    "@tiptap/react": "3.14.0",
    "@tiptap/starter-kit": "3.14.0",
    "@tiptap/suggestion": "3.14.0",
    "@tiptap/y-tiptap": "3.0.1",
    "better-sqlite3": "^12.5.0",
    "bonjour-service": "^1.3.0",
    "chokidar": "^3.6.0",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.19",
    "fastify": "^5.6.2",
    "i18next": "^23.7.0",
    "lowlight": "^3.3.0",
    "node-forge": "^1.3.2",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.0.0",
    "react-resizable-panels": "^3.0.6",
    "sharp": "^0.34.5",
    "y-prosemirror": "^1.3.7",
    "yjs": "^13.6.10"
  },
  "devDependencies": {
    "@electron/rebuild": "^4.0.1",
    "@eslint/js": "^9.17.0",
    "@playwright/test": "^1.56.1",
    "@swc/core": "^1.11.29",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@types/better-sqlite3": "^7.6.8",
    "@types/jest": "^29.5.11",
    "@types/node-forge": "^1.3.14",
    "@types/qrcode": "^1.5.6",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^38.4.0",
    "electron-builder": "^26.3.2",
    "electron-vite": "^4.0.1",
    "eslint": "^9.17.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.56.1",
    "ts-jest": "^29.1.1",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.21.0",
    "vite": "^5.0.10",
    "ws": "^8.18.3"
  }
}
